<title>{{ title }}</title>
<meta name="description" content="{{ description }}" />

<meta charset="utf-8" />
<meta http-equiv="x-ua-compatible" content="ie=edge" />
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
<meta name="robots" content="index, follow" />
<meta name="googlebot" content="index, follow, max-snippet:-1, max-image-preview:large, max-video-preview:-1" />
<meta name="bingbot" content="index, follow, max-snippet:-1, max-image-preview:large, max-video-preview:-1" />
<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=yes" />

<link rel="preload" href="{{ 'Montserrat/Montserrat-Bold.eot?#iefix' | fonts_path }}" as="font" type="font/eot" crossorigin>
<link rel="preload" href="{{ 'Montserrat/Montserrat-Bold.woff' | fonts_path }}" as="font" type="font/woff" crossorigin>
<link rel="preload" href="{{ 'Montserrat/Montserrat-Bold.ttf' | fonts_path }}" as="font" type="font/ttf" crossorigin>
<link rel="preload" href="{{ 'Montserrat/Montserrat-SemiBold.eot?#iefix' | fonts_path }}" as="font" type="font/eot" crossorigin>
<link rel="preload" href="{{ 'Montserrat/Montserrat-SemiBold.woff' | fonts_path }}" as="font" type="font/woff" crossorigin>
<link rel="preload" href="{{ 'Montserrat/Montserrat-SemiBold.ttf' | fonts_path }}" as="font" type="font/ttf" crossorigin>
<link rel="preload" href="{{ 'Montserrat/Montserrat-Medium.eot?#iefix' | fonts_path }}" as="font" type="font/eot" crossorigin>
<link rel="preload" href="{{ 'Montserrat/Montserrat-Medium.woff' | fonts_path }}" as="font" type="font/woff" crossorigin>
<link rel="preload" href="{{ 'Montserrat/Montserrat-Medium.ttf' | fonts_path }}" as="font" type="font/ttf" crossorigin>
<link rel="preload" href="{{ 'Montserrat/Montserrat-Regular.eot?#iefix' | fonts_path }}" as="font" type="font/eot" crossorigin>
<link rel="preload" href="{{ 'Montserrat/Montserrat-Regular.woff' | fonts_path }}" as="font" type="font/woff" crossorigin>
<link rel="preload" href="{{ 'Montserrat/Montserrat-Regular.ttf' | fonts_path }}" as="font" type="font/ttf" crossorigin>
<link rel="preload" href="{{ 'Cousine/Cousine-Bold.eot?#iefix' | fonts_path }}" as="font" type="font/eot" crossorigin>
<link rel="preload" href="{{ 'Cousine/Cousine-Bold.woff' | fonts_path }}" as="font" type="font/woff" crossorigin>
<link rel="preload" href="{{ 'Cousine/Cousine-Bold.ttf' | fonts_path }}" as="font" type="font/ttf" crossorigin>

{{ {} | favicon_tags }}

<meta property="og:title" content="{{ title }}" />
<meta property="og:image" content="{{ socialImage }}" />
<meta property="og:type" content="article" />
<meta property="og:url" content="{{ env.SITE_URI }}{{ page.lang }}" />
<meta property="og:description" content="{{ description }}" />

<meta property="twitter:card" content="summary_large_image" />
<meta property="twitter:url" content="{{ env.SITE_URI }}{{ page.lang }}" />
<meta property="twitter:title" content="{{ title }}" />
<meta property="twitter:description" content="{{ description }}" />
<meta property="twitter:image" content="{{ socialImage }}" />
<meta property="twitter:image:width" content="2000" />
<meta property="twitter:image:height" content="450" />
<meta property="twitter:site" content="Mining24" />

<link rel="stylesheet" media="screen" href="{{ 'bundle.css' | css_path }}" crossorigin />

