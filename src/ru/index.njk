{% macro renderSocialItems(entry) -%}
  <div class="social-item">
    <a class="social-item__link" target="_blank" href="{{ entry.link }}">
      <i class="social-item__icon -{{ entry.slug }}"></i>
    </a>
  </div>
{%- endmacro %}

