            </div>
          </div>
          <div class="text-wrap">
            <p class="text">Начните со $100</p>
          </div>
          <div class="show-for-small social">
            {%- for entry in data.layout.socials %}{{ renderSocialItems(entry) }}{%- endfor -%}
          </div>
        </div>
        <div class="cell small-12 large-6 xlarge-6 right-side">
          <div class="animation-wrap">
            <img class="animation" srcset="/assets/images/tree.png, /assets/images/tree@2x.png 2x" src="/assets/images/tree.png" alt="tree" />
